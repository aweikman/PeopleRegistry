{
  "request": {
    "method": "POST",
    "url": "/people",
    "headers" : {
      "Authorization" : {
        "contains" : "i am a session token"
      }
    },
    "bodyPatterns" : [ {
      "equalToJson" : "{\"person_first_name\":\"Lou\",\"person_last_name\":\"Smith\",\"dob\":\"1993-02-19\"}",
      "ignoreArrayOrder" : true,
      "ignoreExtraElements" : true
    } ]
  },
  "response": {
    "status": 200,
    "body": "{\"id\":4}",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}